n\\r\\n    <div class=\\\"uk-overflow-container\\\">\\r\\n        <table class=\\\"uk-table uk-table-striped uk-text-nowrap\\\">\\r\\n            <thead>\\r\\n                <tr>\\r\\n                    <th>Option<\\/th>\\r\\n                    <th>Possible value<\\/th>\\r\\n                    <th>Default<\\/th>\\r\\n                    <th>Description<\\/th>\\r\\n                <\\/tr>\\r\\n            <\\/thead>\\r\\n            <tbody>\\r\\n                <tr>\\r\\n                    <td><code>format<\\/code>\\r\\n                    <\\/td>\\r\\n                    <td>\'24h\' or \'12h\'<\\/td>\\r\\n                    <td>\'24h\'<\\/td>\\r\\n                    <td>Defines the preferred time notation<\\/td>\\r\\n                <\\/tr>\\r\\n                <tr>\\r\\n                    <td><code>start<\\/code>\\r\\n                    <\\/td>\\r\\n                    <td>Integer between 0 and 24<\\/td>\\r\\n                    <td>0<\\/td>\\r\\n                    <td>Start time<\\/td>\\r\\n                <\\/tr>\\r\\n                <tr>\\r\\n                    <td><code>end<\\/code>\\r\\n                    <\\/td>\\r\\n                    <td>Integer between 0 and 24<\\/td>\\r\\n                    <td>24<\\/td>\\r\\n                    <td>End time<\\/td>\\r\\n                <\\/tr>\\r\\n            <\\/tbody>\\r\\n        <\\/table>\\r\\n    <\\/div>\\r\\n\\r\\n    <h3 class=\\\"tm-article-subtitle\\\">Init element manually<\\/h3>\\r\\n\\r\\n    <pre><code class=\\\"javascript\\\"><span class=\\\"keyword\\\">var<\\/span> timepicker = UIkit.timepicker(element, { <span class=\\\"comment\\\">\\/* options *\\/<\\/span> });<\\/code><\\/pre>\\r\\n\",\"fulltext\":\"\",\"state\":1,\"catid\":\"9\",\"created\":\"2016-04-25 08:14:47\",\"created_by\":\"579\",\"created_by_alias\":\"\",\"modified\":\"2016-04-25 08:14:47\",\"modified_by\":null,\"checked_out\":null,\"checked_out_time\":null,\"publish_up\":\"2016-04-25 08:14:47\",\"publish_down\":\"0000-00-00 00: